{
  "name": "ateria-idle",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/ateria-idle",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/ateria-idle"],
      "options": {
        "command": "nuxt generate && mkdir -p ../../dist/apps/ateria-idle && cp -R .output/public/. ../../dist/apps/ateria-idle/",
        "cwd": "apps/ateria-idle"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt dev --port 3001 --host 127.0.0.1",
        "cwd": "apps/ateria-idle"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt preview",
        "cwd": "apps/ateria-idle"
      }
    }
  },
  "tags": ["type:app", "framework:nuxt"]
}
