{
  "name": "main-page",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/main-page",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/main-page"],
      "options": {
        "command": "vite build",
        "cwd": "apps/main-page"
      }
    },
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "main-page:build"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "options": {
        "buildTarget": "main-page:build"
      }
    }
  },
  "tags": ["type:app", "framework:vanilla"]
}
